  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:app": "tsc -b tsconfig.app.json",
    "build:node": "tsc -b tsconfig.node.json",
    "build": "npm run build:app && npm run build:node && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
